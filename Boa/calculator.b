// Calculator (+,-,*,/)
[
    //Get Input
    :-$
    *-$
    &-a
    %-$
    *-$
    &-o
    :-$
    *-$
    &-b
    //Define op value and newline
    @-$
    43-c
    45-d
    47-e
    42-f
    128-l
    //Check Op
    !-$
    (o.c){
        #-$
        b.a
    }
    !-$
    (o.d){
        #-$
        b,a
    }
    !-$
    (o.e){
        #-$
        b;a
    }
    !-$
    (o.f){
        #-$
        b:a
    }
    //Print Result
    &-$
    a/
    l/
]